{
  "name": "vs-mdallinpicgo",
  "displayName": "VS-MDAllInPicGo",
  "description": "基于PicGo的一键上传MD中的所有本地图片到图床",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-mdallinpicgo.uploadImages",
        "title": "上传并替换 Markdown 中的本地图片到图床"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == markdown",
          "command": "vs-mdallinpicgo.uploadImages",
          "group": "navigation@1"
        }
      ]
    },
    "configuration": {
      "title": "VS-MDAllInPicGo",
      "properties": {
        "vs-mdallinpicgo.maxRetries": {
          "type": "number",
          "default": 3,
          "description": "上传失败时的最大重试次数"
        },
        "vs-mdallinpicgo.uploadInterval": {
          "type": "number",
          "default": 2000,
          "description": "上传图片的间隔时间(毫秒)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "ts-loader": "^9.2.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "picgo": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kenis1108/vs-mdallinpicgo.git"
  },
  "publisher": "kenis1108"
}