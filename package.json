{
  "name": "vs-mdallinpicgo",
  "displayName": "vs-mdallinpicgo",
  "description": "一键上传MD中的所有本地图片到图床",
  "version": "0.0.1",
  "engines": {
    "vscode": "^0.10.5"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-mdallinpicgo.uploadImages",
        "title": "上传并替换 Markdown 中的本地图片到 Github 图床"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == markdown",
          "command": "vs-mdallinpicgo.uploadImages",
          "group": "navigation@1"
        }
      ]
    },
    "configuration": {
      "title": "One PicGo",
      "properties": {
        "vs-mdallinpicgo.config": {
          "type": "object",
          "default": {
            "uploader": "github",
            "github": {
              "repo": "kenis1108/imgbed",
              "token": "",
              "path": "test/",
              "customUrl": "https://cdn.jsdelivr.net/gh/kenis1108/imgbed@main",
              "branch": "main"
            }
          },
          "description": "PicGo 图床配置"
        },
        "vs-mdallinpicgo.uploadInterval": {
          "type": "number",
          "default": 1000,
          "description": "每张图片上传间隔时间(毫秒)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "electron": "^32.1.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.9.1",
    "npm-run-all": "^4.1.5",
    "proxy-agent": "^6.4.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "picgo": "^1.5.7"
  }
}